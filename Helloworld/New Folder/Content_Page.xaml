<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Helloworld.Content_Page" Title="Home" xmlns:local="clr-namespace:Helloworld;assembly:Helloworld">
	<ContentPage.Content>
		<StackLayout x:Name="OuterStackView" Margin="10" Orientation="Vertical" VerticalOptions="FillAndExpand">
			<StackLayout Spacing="10" Orientation="Horizontal" HorizontalOptions="FillAndExpand" HeightRequest="100">
				<BoxView BackgroundColor="Black" HorizontalOptions="Start" WidthRequest="100">
				</BoxView>
				<BoxView BackgroundColor="Black" HorizontalOptions="FillAndExpand">
				</BoxView>
			</StackLayout>
			<BoxView BackgroundColor="Black" HorizontalOptions="End">
			</BoxView>
			<BoxView BackgroundColor="Black" HorizontalOptions="Center" WidthRequest="200">
			</BoxView>
			<BoxView BackgroundColor="Black" HorizontalOptions="Start">
			</BoxView>
			<Button x:Name="login" Text="Next Page" BackgroundColor="Gray" BorderWidth="2" BorderColor="Fuchsia" BorderRadius="15" TextColor="White" Clicked="Handle_Clicked">
			</Button>
			<local:MyEntry Text="My Entry"></local:MyEntry>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>